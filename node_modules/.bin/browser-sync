#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/browser-sync@3.0.4/node_modules/browser-sync/dist/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/browser-sync@3.0.4/node_modules/browser-sync/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/browser-sync@3.0.4/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/browser-sync@3.0.4/node_modules/browser-sync/dist/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/browser-sync@3.0.4/node_modules/browser-sync/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/browser-sync@3.0.4/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../browser-sync/dist/bin.js" "$@"
else
  exec node  "$basedir/../browser-sync/dist/bin.js" "$@"
fi
