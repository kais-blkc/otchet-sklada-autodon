#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/stream-throttle@0.1.3/node_modules/stream-throttle/bin/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/stream-throttle@0.1.3/node_modules/stream-throttle/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/stream-throttle@0.1.3/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/stream-throttle@0.1.3/node_modules/stream-throttle/bin/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/stream-throttle@0.1.3/node_modules/stream-throttle/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/stream-throttle@0.1.3/node_modules:/Users/Apple/work/otchet-sklada-new/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../stream-throttle/bin/throttleproxy.js" "$@"
else
  exec node  "$basedir/../stream-throttle/bin/throttleproxy.js" "$@"
fi
